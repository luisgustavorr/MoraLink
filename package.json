{
  "name": "MoraLink",
  "version": "1.2.53",
  "repository": {
    "type": "git",
    "url": "https://github.com/luisgustavorr/MoraLink.git"
  },
  "icon": "./app/assets/images/favicon_io/favicon.ico",
  "description": "Integração com o Shark Business",
  "main": "./app.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --linux -p always",
    "build:win": "electron-builder --win --x64 -p always",
    "build:win32": "electron-builder --win --ia32 -p always --config.directories.output=dist/win32",
    "build:win:all": "npm run build:win && npm run build:win32"
  },
  "build": {
    "files": "!build_assets{/*}",
    "appId": "com.electron.moralink",
    "forceCodeSigning": true,
    "extraResources": [
      ".env"
    ],
    "win": {
      "publish": [
        {
          "provider": "github",
          "owner": "luisgustavorr",
          "repo": "MoraLink"
        }
      ],
      "target": [
        "nsis"
      ],
      "certificateFile": "build_assets/cert.pfx",
      "certificatePassword": "galo1313",
      "icon": "app/assets/images/favicon_io/favicon.ico"
    },
    "nsis": {
      "oneClick": true,
      "deleteAppDataOnUninstall": true,
      "removeDefaultUninstallWelcomePage": false,
      "installerIcon": "./app/assets/images/favicon_io/favicon.ico",
      "uninstallerIcon": "./app/assets/images/favicon_io/favicon.ico",
      "uninstallDisplayName": "MoraLink",
      "license": "./app/assets/license.md",
      "allowToChangeInstallationDirectory": false
    }
  },
  "author": "ORBIS SOLUCOES TECNOLOGICAS",
  "license": "ISC",
  "dependencies": {
    "ansi-colors": "^4.1.3",
    "auto-launch": "^5.0.6",
    "axios": "^1.7.7",
    "body-parser": "^1.20.2",
    "cli-progress": "^3.12.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.1.8",
    "express": "^4.18.3",
    "http-proxy": "^1.18.1",
    "jsonic": "^2.15.2",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mssql": "^11.0.1",
    "multer": "^1.4.5-lts.1",
    "mysql": "2.14.1",
    "mysql-connection-pool-manager": "^1.5.9",
    "node-cron": "^3.0.3",
    "node-firebird": "^1.1.9",
    "pdf-parse": "^1.1.1",
    "pg": "^8.13.1",
    "pg-cursor": "^2.15.3",
    "socks-proxy-agent": "^8.0.5",
    "ssh-tunneling": "^1.1.23",
    "ssh2": "^1.16.0",
    "ssh2-promise": "^1.0.3",
    "tcp-port-used": "^1.0.2",
    "winston": "^3.17.0",
    "ws": "^8.18.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "babel-eslint": "^10.1.0",
    "electron": "22.1.0",
    "electron-builder": "^24.13.3",
    "eslint": "^9.17.0",
    "globals": "^15.14.0"
  },
  "resolutions": {
    "mysql": "2.14.1"
  }
}
